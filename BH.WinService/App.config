<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6"/>
  </startup>
  <appSettings>
    <add key="EnableRelSearch" value="True"/>
    <add key="IndexCurrentMonth" value="True"/>
    <add key="EnableRobots" value="True"/>
    <add key="BoobenMode" value="True"/> <!-- temporary flag -->
    <add key="IndexPath" value="Indexes"/>
    <add key="LimitUsedMemory" value="16000000000"/>
    <add key="InMemoryMode" value="True"/>
    <add key="URL" value="http://localhost:5056"/>
  </appSettings>
</configuration>
